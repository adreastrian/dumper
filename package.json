{
  "name": "symfony-dump-viewer",
  "version": "1.0.0",
  "description": "A beautiful debugging tool that captures and displays PHP dump() output with Laravel Herd-style interface",
  "main": "dist/index.js",
  "bin": {
    "dump-viewer": "./bin/dump-viewer.js",
    "sdv": "./bin/dump-viewer.js",
    "install-php-helper": "./bin/install-php-helper.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:server": "nodemon dist/index.js --no-open",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start": "node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "php",
    "debugging",
    "dump",
    "symfony",
    "laravel",
    "var-dump",
    "development"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "commander": "^11.1.0",
    "open": "^9.1.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/ws": "^8.5.8",
    "typescript": "^5.2.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}